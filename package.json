{
  "name": "cleanwater-scan",
  "version": "1.0.0",
  "description": "AI-powered water quality analysis and monitoring platform",
  "author": "CleanWater Technologies <team@cleanwater-scan.com>",
  "license": "MIT",
  "homepage": "https://cleanwater-scan.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cleanwater/scan.git"
  },
  "bugs": {
    "url": "https://github.com/cleanwater/scan/issues"
  },
  "keywords": [
    "water-quality",
    "ai-analysis",
    "environmental-monitoring",
    "nextjs",
    "tensorflow",
    "computer-vision",
    "pwa",
    "health",
    "safety"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,md,json}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,md,json}\"",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:pull": "prisma db pull",
    "db:seed": "prisma db seed",
    "db:reset": "prisma migrate reset --force",
    "db:setup": "npm run db:generate && npm run db:push && npm run db:seed",
    "db:studio": "prisma studio",
    "setup": "ts-node scripts/setup.ts",
    "prepare": "husky install",
    "postinstall": "npm run db:generate",
    "analyze": "cross-env ANALYZE=true next build",
    "export": "next export",
    "clean": "rm -rf .next out node_modules/.cache",
    "update-deps": "npm update && npm audit fix"
  },
  "dependencies": {
    "next": "14.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "^5.0.0",
    "tailwindcss": "3.3.0",
    "autoprefixer": "10.4.16",
    "postcss": "8.4.31",
    "@tailwindcss/forms": "0.5.7",
    "class-variance-authority": "0.7.0",
    "clsx": "2.0.0",
    "tailwind-merge": "1.14.0",
    "lucide-react": "0.290.0",
    "next-auth": "4.24.5",
    "bcryptjs": "2.4.3",
    "@prisma/client": "5.6.0",
    "prisma": "5.6.0",
    "@tensorflow/tfjs": "4.13.0",
    "@tensorflow/tfjs-node": "4.13.0",
    "react-apexcharts": "1.4.1",
    "apexcharts": "3.44.0",
    "date-fns": "2.30.0",
    "framer-motion": "10.16.5",
    "jspdf": "2.5.1",
    "html2canvas": "1.4.1",
    "@radix-ui/react-toast": "1.1.5",
    "@radix-ui/react-progress": "1.0.3",
    "@radix-ui/react-switch": "1.0.3",
    "@radix-ui/react-select": "2.0.0",
    "recharts": "2.8.0",
    "sharp": "0.32.6",
    "zod": "3.22.4",
    "react-hook-form": "7.47.0",
    "@hookform/resolvers": "3.3.2",
    "sonner": "1.2.0",
    "cmdk": "0.2.0"
  },
  "devDependencies": {
    "@types/node": "20.8.0",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0",
    "@types/bcryptjs": "2.4.6",
    "eslint": "8.51.0",
    "eslint-config-next": "14.0.0",
    "prettier": "3.0.3",
    "@typescript-eslint/eslint-plugin": "6.9.0",
    "@typescript-eslint/parser": "6.9.0",
    "jest": "29.7.0",
    "@testing-library/react": "14.0.0",
    "@testing-library/jest-dom": "6.1.4",
    "jest-environment-jsdom": "29.7.0",
    "@next/bundle-analyzer": "14.0.0",
    "cross-env": "7.0.3",
    "husky": "8.0.3",
    "lint-staged": "15.0.2",
    "ts-node": "10.9.1",
    "@playwright/test": "1.40.0"
  },
  "prisma": {
    "seed": "ts-node scripts/seed.ts"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
} 